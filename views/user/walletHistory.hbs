
   <script src="/javascripts/ajax.js"></script>     
       
       
        <main class="main">
        	<div class="page-header text-center" style="background-image: url('assets/images/page-header-bg.jpg')">
        		<div class="container">
        			<h1 class="page-title">My Wallet<span>Total Amount: {{walletAmount}}</span></h1>
        		</div><!-- End .container -->
        	</div><!-- End .page-header -->
           

            <div class="page-content">
            	<div class="container">
					<table class="table table-wishlist table-mobile">
						<thead >
							<tr >
								<th style="padding: 30px;">Order ID</th>
                                
								
                               
                                <th style="padding: 30px;">Amount paid</th>
                                <th style="padding: 30px;">Credit/Debit Amount</th>
                                
                                <th style="padding: 30px;">Payment method</th>
								
								<th style="padding: 30px;">Order Date</th>
                                <th style="padding: 30px;">Credit/Debit Date</th>

								
							</tr>
						</thead>

						<tbody style="padding:;">
                            {{#each walletDetails}}
							<tr class="" > 
                                    <td class="price-col" style="padding: 30px;">Order {{this.orderId}}</td>
                                    <td class="price-col" style="padding: 30px;">₹{{this.amountPaid}}</td>
                                    {{#if this.creditAmount}}
                                    <td class="price-col text-success " style="padding: 30px;">₹{{this.creditAmount}}</td>
                                    {{else}}
                                    <td class="price-col text-danger" style="padding: 30px;">₹{{this.debitAmount}}</td>
                                    {{/if}}
                                    <td class="price-col" style="padding: 30px;">{{this.paymentMethod}}</td>
								
								<td class="action-col" style="padding: 30px;">{{orderDate}}</td>
                                {{#if this.creditDate}}
                                <td class="action-col text-success" style="padding: 30px;">{{this.creditDate}}</td>
                                {{else}}
                                <td class="action-col text-danger" style="padding: 30px;"> {{this.debitDate}}</td>
                                {{/if}}
                              
							</tr>
                        {{/each}}
						</tbody>
					</table><!-- End .table table-wishlist -->





	            	<div class="wishlist-share">
	            		<div class="social-icons social-icons-sm mb-2">
	            			<label class="social-label">Share on:</label>
	    					<a href="#" class="social-icon" title="Facebook" target="_blank"><i class="icon-facebook-f"></i></a>
	    					<a href="#" class="social-icon" title="Twitter" target="_blank"><i class="icon-twitter"></i></a>
	    					<a href="#" class="social-icon" title="Instagram" target="_blank"><i class="icon-instagram"></i></a>
	    					<a href="#" class="social-icon" title="Youtube" target="_blank"><i class="icon-youtube"></i></a>
	    					<a href="#" class="social-icon" title="Pinterest" target="_blank"><i class="icon-pinterest"></i></a>
	    				</div><!-- End .soial-icons -->
	            	</div><!-- End .wishlist-share -->
            	</div><!-- End .container -->
            </div><!-- End .page-content -->
        </main><!-- End .main -->


